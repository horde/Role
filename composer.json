{
    "name": "horde/role",
    "description": "PEAR installer role used to install Horde components",
    "type": "library",
    "homepage": "https://pear.horde.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        },
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        }
    ],
    "version": "1.0.2",
    "time": "2017-10-17",
    "repositories": [
        {
            "type": "pear",
            "url": "https://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3 || ^7"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Role": "1.*",
        "pear-horde/Horde_Role": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Role": "lib/"
        }
    }
}
